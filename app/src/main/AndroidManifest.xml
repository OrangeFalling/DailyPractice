<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wine.practice">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyPractice"
        tools:targetApi="31">
        <activity
            android:name="com.wine.practice.binding.BindingUsing"
            android:exported="false" />
        <activity
            android:name="com.wine.practice.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wine.practice.bar.ImmersiveBar"
            android:exported="false" />
        <activity android:name="com.wine.practice.surface.SurfaceActivity"
            android:exported="false"/>
    </application>

</manifest>